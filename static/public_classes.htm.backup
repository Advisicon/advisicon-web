		   		<h2 style="font-size:16px;">Current Course Offerings</h2>
				<%@LANGUAGE=VBSCRIPT%>
				<%

				Dim strConnection, conn, rs, strSQL 

				strConnection = "Provider=SQLOLEDB;Server=ADVISICON-COLO;Database=AdvisiconCourses;User Id=webuser; password=webuser;encrypt=true" 

				Set conn = Server.CreateObject("ADODB.Connection")

				conn.Open strConnection

				Set rs = Server.CreateObject("ADODB.recordset")
				strSQL = "SELECT dbo.Courses.CourseName AS [Course Title], dbo.Classes.ClassLocName AS Location, dbo.Classes.ClassCity + ', ' + UPPER(LEFT(dbo.Classes.ClassState,2)) AS [City & State], dbo.Classes.ClassStartDate AS Date, dbo.Classes.ClassLink AS Link FROM dbo.Courses, dbo.Classes WHERE dbo.Courses.CourseID = dbo.Classes.ClassTitle AND dbo.Classes.ClassStartDate > GETDATE() AND LTRIM(RTRIM(CurStatus)) = 'Active' AND LTRIM(RTRIM(RegStatus))='Open' ORDER BY dbo.Classes.ClassStartDate ASC"
				rs.open strSQL, conn, 3,3
				
				%>
				
				<table border="1" width="95%">
				<tr>
				<%
				
						for each x in rs.Fields
							response.write("<th>" & x.name & "</th>")
						next
						%>
						</tr>
				
						<% do until rs.EOF %>

						<tr>
				
						<%
						for each x in rs.Fields
					
							Select Case x.name
								Case "Course Title"
								response.write("<td width='40%' style='font-size:14px; margin-top:10px;padding-top:10px'>" & x.value & "</td>")
								Case "Location"
								response.write("<td width='10%' style='font-size:14px; margin-top:10px;padding-top:10px'>" & x.value & "</td>")
								Case "City & State"
								response.write("<td width='15%' style='font-size:14px; margin-top:10px;padding-top:10px'>" & x.value & "</td>")
								Case "Date"
								response.write("<td width='10%' style='font-size:14px; margin-top:10px;padding-top:10px'>" & x.value & "</td>")
								Case "Link"
								response.write("<td width='10%' style='font-size:14px; margin-top:10px;padding-top:10px'><a href='" & x.value & "' target='_blank'>Information</a>")
		
							End Select					
				
						next
						%>
				
						<%rs.MoveNext%>
					
				</tr>
				<% loop %>
				</table>
				
				<%
				
				rs.Close
				Set rs = Nothing

				conn.Close
				Set conn = Nothing
				%>
				
